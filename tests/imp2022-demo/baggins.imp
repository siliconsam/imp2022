%begin
    %external %routine           %spec zerocount %alias "xzero"
    %external %routine           %spec increment %alias "xinc"
    %external %routine           %spec decrement %alias "xdec"
    %external %integer %function %spec value %alias "xvalue"

    %external %routine           %spec dumpalltrapinfo
    %integer i

    set debug level( dbgtrap )
    %if (debugmode = dbgtrap) %then dumpalltrapinfo

    %for i = 1,1,10 %cycle
        increment
    %repeat

    debugstring("Value = ".itos(value,0)); debugnewline

%end
%endoffile
